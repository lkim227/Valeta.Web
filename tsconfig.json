    "moduleResolution": "node",
    "types": ["angular", "jquery", "lodash", "angular-ui-router", "signalr"],
    "typeRoots": ["./node_modules/@types"],
    "esModuleInterop": true
  },
  "include": [
    "appCommon/**/*",
    "lib/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
